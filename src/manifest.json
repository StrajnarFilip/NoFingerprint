{
  "description": "Prevent websites from getting useful fingerprints.",
  "manifest_version": 2,
  "name": "NoFingerprint",
  "version": "1.0",
  "homepage_url": "https://github.com/StrajnarFilip/NoFingerprint.git",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}